{
  "compileOnSave": true,
  "compilerOptions": {
    "experimentalDecorators": true,
    "emitDecoratorMetadata": true,
    "removeComments": false,
    "moduleResolution": "node",
    "target": "es2015",
    "lib": [
      "dom",
      "es2017",
      "esnext.asynciterable"
