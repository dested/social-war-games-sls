{
  "name": "swg-api-user",
  "version": "1.0.0",
  "description": "",
  "scripts": {
    "start": "node ./dist/index.js",
    "start-ts": "node ./run.js",
    "start:watch": "nodemon",
    "deploy": "npm run tsc & eb deploy",
    "test": "npx jest",
    "tsc": "npx tsc",
    "pretty": "npx prettier \"*/**/*.ts*\"  --config .prettierrc --write",
    "lint": "npx tslint -p tsconfig.json --fix",
    "pre": "yarn tsc & yarn pretty & yarn lint",
    "dev": "cross-env NODE_ENV=development webpack --watch",
    "build": "cross-env NODE_ENV=production webpack --progress --hide-modules"
  },
  "dependencies": {
    "@nestjs/common": "^4.6.6",
    "@nestjs/core": "^4.6.6",
    "@nestjs/microservices": "^4.6.6",
    "@nestjs/testing": "^4.6.6",
    "@nestjs/websockets": "^4.6.6",
    "aws-sdk": "^2.218.1",
    "bcryptjs": "^2.4.3",
    "esprima": "^4.0.0",
    "jsonwebtoken": "^8.1.0",
    "moment": "^2.22.0",
    "passport": "^0.4.0",
    "passport-jwt": "^3.0.1",
    "redis": "^2.7.1",
    "reflect-metadata": "^0.1.12",
    "rxjs": "^5.5.8",
    "typescript": "^2.8.3",
    "uuid": "^3.2.1"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.1",
    "@types/esprima": "^4.0.1",
    "@types/express": "^4.11.1",
    "@types/jest": "^22.2.3",
    "@types/jsonwebtoken": "^7.2.5",
    "@types/node": "^8.0.28",
    "@types/passport": "^0.4.2",
    "@types/passport-jwt": "^3.0.0",
    "jest": "^22.4.3",
    "nodemon": "^1.12.1",
    "prettier": "^1.11.1",
    "ts-jest": "^22.4.2",
    "ts-node": "^3.3.0",
    "tslint": "^5.9.1",
    "tslint-config-prettier": "^1.10.0",
    "tslint-eslint-rules": "^5.1.0",
    "tslint-plugin-prettier": "^1.3.0",
    "typescript": "^2.8.3",
    "ts-loader": "^3.0.5",
    "webpack": "^3.8.1"
  },
  "jest": {
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(tsx?)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  }
}
